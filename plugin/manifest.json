{
  "id": "RRM.Utils",
  "name": "RRM Utilties",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "ID",
      "minVersion": "19.1.0"
    }
  ],
  "featureFlags": {
    "enableSWCSupport": true
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "RRM.VariablePanel",
      "label": {
        "default": "Variables"
      },
      "minimumSize": {
        "width": 300,
        "height": 200
      },
      "maximumSize": {
        "width": 400,
        "height": 1000
      },
      "preferredDockedSize": {
        "width": 400,
        "height": 500
      },
      "preferredFloatingSize": {
        "width": 400,
        "height": 500
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/RRM.png",
          "scale": [1, 2, 1.5, 1.25],
          "theme": ["darkest", "dark", "medium", "light", "lightest", "all"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/RRMGray.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
      "species": ["pluginList"]
    }
  ],
  "runOnStartup": true
}
